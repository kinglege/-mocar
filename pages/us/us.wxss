/* pages/us/us.wxss */

.container {
  background-color: #f7f7f7;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

page {
  background-color: #f7f7f7;
}

/* 记录 */

.book {
  background-color: #fff;
  display: flex;
  justify-content: space-around;
  align-items: center;
  align-self: auto;
  margin-top: 17rpx;
}

.book_first {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 33.33%;
  height: 128rpx;
  /* padding-top: 10rpx; *//* text-align: center;
  margin: auto;
  line-height: 52rpx; */
}

.book_first .book_num {
  height: 50rpx;
  color: rgb(34, 34, 34);
  font-weight: 700;
  font-size: 28rpx;
}

.book_first .book_name {
  font-size: 28rpx;
}

/* 订单 */

.order {
  background-color: #fff;
  margin-top: 17rpx;
  padding: 0 37rpx;
  height: 85rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.order_us {
  font-size: 28rpx;
  color: rgb(34, 34, 34);
  font-weight: 700;
}

.order_every {
  font-size: 24rpx;
}

.order_content_text {
  margin-top: 10rpx;
  font-size: 20rpx;
}

/* 订单内容 */

.order_content {
  background-color: #fff;
  display: flex;
  height: 125rpx;
  padding: 0 0rpx;
  margin-top: 2rpx;
}

.order_content_first {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 25%;
  height: 125rpx;
}

.order_content_image {
  width: 45rpx;
  height: 45rpx;
}

.order_content_image image {
  width: 45rpx;
  height: 45rpx;
}

/* 分类 */

.ification {
  height: 300rpx;
  background-color: #fff;
  display: flex;
  flex-wrap: wrap;
  margin-top: 17rpx;
}

.ification_first {
  width: 50%;
  height: 100rpx;
  border-bottom: 3rpx solid #f7f7f7;
  display: flex;
  justify-content: center;
  align-items: center;
}

.ification_first:nth-child(2n+1) {
  border-right: 3rpx solid #f7f7f7;
}

.ification_first:nth-child(7) {
  border-bottom: none;
}

.ification_first:nth-child(8) {
  border-bottom: none;
}

.ification_image {
  width: 46rpx;
  height: 46rpx;
  margin-right: 20rpx;
}

.ification_image image {
  width: 46rpx;
  height: 46rpx;
}

.ification_name {
  width: 30%;
  font-size: 24rpx;
}

.ification_names {
  font-size: 24rpx;
}

.ll {
  width: 100%;
  height: 100%;
  background-color: #f7f7f7;
  overflow-x: hidden;
  overflow-y: auto;
}

/* 头部 */

.UCenter-bg {
  background-image: url(data:image/png;base64,);
  /* background-color: lime; */
  background-size: cover;
  height: 380rpx;
  display: flex;
  justify-content: center;
  padding-top: 40rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: flex-start;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.bg_img {
  display: flex;
  margin-left: 73rpx;
  justify-content: center;
  align-items: center;
}

.text-xl {
  padding-left: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg image {
  width: 100rpx;
  height: 100rpx;
}

.UCenter-bg .gif-wave {
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;
  height: 100rpx;
}

map, .mapBox {
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;
  height: 100rpx;
}

map, .mapBox {
  width: 750rpx;
  height: 300rpx;
}
