/* pages/ticket/ticket.wxss */

page {
  margin-bottom: 100rpx;
}

.screen-swiper {
  height: 746rpx;
}
.info_right{
  justify-content:space-between;
}
.info_right_img {
  width: 40rpx;
  height: 40rpx;
 margin-bottom:10rpx;
}
.btn{
  margin: 0;
  padding: 0;
  background-color: #fff;
 
}
.btn::after{
  border:none;
}
.info_right_img image {
  width: 40rpx;
  height: 40rpx;
}
.info_top{
  justify-content: space-between;
}
.ticket_info {
  height: 196rpx;
  background-color: #fff;
  padding-top: 21rpx;
  
}

.info_time {
  margin-top: 20rpx;
  font-size: 24rpx;
}

.wash {
  font-size: 28rpx;
  font-weight: 700;
  color: rgb(34, 34, 34);
}

.p1 {
  font-size: 32rpx;
  color: rgb(247, 67, 66);
}

.p2 {
  font-size: 24rpx;
  line-height: 48rpx;
  text-decoration: line-through;
}

.p3 {
  font-size: 24rpx;
}

/* 领取 */

.receive {
  height: 100rpx;
  margin-top: 17rpx;
}

.receive_new {
  font-size: 24rpx;
}

.receive_collar {
  font-size: 22rpx;
  line-height: 40rpx;
  text-align: center;
  border-radius: 6rpx;
  width: 110rpx;
  height: 40rpx;
  background-color: rgb(247, 67, 66);
  color: rgb(255, 255, 255);
}

/* logo部分布局 */
.store_right{
  width: 15rpx;
  height: 23rpx;
}
.store_right image{
  width: 15rpx;
  height: 23rpx;
}
.detail_all {
  padding: 0 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 15rpx;
  width: 100%;
  height: 150rpx;
  background-color: #fff;
}

.detail_img {
  margin-right: 24rpx;
  width: 90rpx;
  height: 90rpx;
  
}

.detail_img image {
  width: 90rpx;
  height: 90rpx;
}

.detail_top {
  display: flex;
  flex-direction: column;

  /* text-align: center; */
}

.text_top {
  height: 30rpx;
  line-height: 30rpx;
  margin-bottom: 20rpx;
}

.text_top .txt1 {
  font-size: 24rpx;
  color: rgb(24, 24, 24);
  letter-spacing: 1rpx;
  padding-right: 10rpx;
}

.text_top .txt2 {
  font-size: 20rpx;
  color: rgb(191, 191, 191);
  letter-spacing: 1rpx;
}

.txt3 {
  font-size: 20rpx;
  letter-spacing: 1rpx;
  padding-top: 20rpx;
  height: 29rpx;
  line-height: 0rpx;
}

/* 联系部分 */

.contact {
  background-color: #fff;
  display: flex;
  width: 100%;
  height: 66rpx;
  margin-top: 3rpx;
}

.contact_left {
  height: 66rpx;
  width: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.img_left {
  width: 30rpx;
  height: 30rpx;
  margin-right: 15rpx;
}

.img_left image {
  width: 30rpx;
  height: 30rpx;
}

.severce {
  display: flex;
  background-color: #fff;
  justify-content: center;
  align-items: center;
  margin-top: 15rpx;
  color: rgb(186, 186, 186);
  font-size: 28rpx;
  height: 116rpx;
}

.article {
  margin-top: 15rpx;
  background-color: #fff;
  /* padding: 0 20rpx; */
}

.article01 {
  font-size: 28rpx;
  color: rgb(34, 34, 34);
  font-family: PingFang-SC-Medium;
}

.article02 {
  color: rgb(102, 102, 102);
  font-size: 28rpx;
  margin-top: 22rpx;
}

.article03 {
  color: rgb(102, 102, 102);
  font-size: 28rpx;
  margin-top: 22rpx;
}

.article04 {
  font-size: 28rpx;
  color: rgb(34, 34, 34);
  margin-top: 43rpx;
  font-family: PingFang-SC-Medium;
}

.article05 {
  color: rgb(102, 102, 102);
  font-size: 28rpx;
  margin-top: 20rpx;
}

/* 底部图片 */

.footer_img {
  width: 700rpx;
  height: 400rpx;
  margin-top: 40rpx;
  border-radius: 15rpx;
}

.footer_img image {
  width: 700rpx;
  height: 400rpx;
  border-radius: 15rpx;
}

/* 底布标签 */

.bar {
  width: 100%;
  padding: 0 20rpx;
  justify-content: space-around;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  border-top:1rpx solid #ddd;
  background-color: #fff;
}

.bar_left_img {
  width: 40rpx;
  height: 40rpx;
}

.bar_left_img image {
  width: 40rpx;
  height: 40rpx;
}
.details_right{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.bar_left_txt {
  font-size: 24rpx;
  border: none;
  background: white;
  /* width: 40rpx;
  height: 20rpx; */
  line-height: 20rpx;
}
.contact-btn{
  display: flex;
  align-items: center;
  justify-content: center;
  background: none;
  border: 1rpx solid white;
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.contact-btn::after{
    border: 1rpx solid white;
}
.bar_right {
  width: 380rpx;
  height: 80rpx;
  background-color: rgb(233, 38, 64);
  font-size: 36rpx;
  line-height: 80rpx;
  text-align: center;
  transform: translateX(30rpx);
  border-radius: 10rpx;
  color: rgb(255, 255, 255);
}

/* 模态框 */
.coupon{
  border-top-right-radius: 20rpx;
  border-top-left-radius: 30rpx;
}
.coupon_modl {
  background-color: #f7f7f7;
}

.effective {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 710rpx;
  margin-bottom: 20rpx;
  height: 200rpx;
  margin-left: 20rpx;
  background-repeat: no-repeat;
  background-size: cover;
}

.effective_info {
  padding: 40rpx 0rpx 40rpx 30rpx;
  text-align: left;
}

.info_name {
  font-size: 32rpx;
  color: rgb(17, 17, 17);
}

.info_type {
  font-size: 24rpx;
  margin-top: 15rpx;
}

.info_time {
  font-size: 22rpx;
  margin-top: 23rpx;
}

.info_price {
  display: flex;
  font-size: 80rpx;
  font-weight: 700;
  color: #fff;
  margin-right: 50rpx;
  justify-content: center;
  align-items: center;
}

.info_price .qq {
  font-size: 36rpx;
  line-height: 200rpx;
  padding-left: 40rpx;
  font-weight: 700;
  /* padding-top: 33rpx; */
}

