/* pages/home/search_car/search_car.wxss */
.page_inp{
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
  padding: 0 20rpx;
}
page{
  background-color: #fff;
}
.text-input{
  line-height: 80rpx;
  height: 80rpx;
  padding:0 30rpx; 
}
.text_inp{
  margin-top: 20rpx;
  width: 608rpx;
  /* line-height: 80rpx; */
  height: 80rpx;
  background-color: rgb(247,247,247);
  border-radius: 80rpx;
}
.searchInput{
  width:100rpx;
  border: 1rpx solid #fff;
  background-color: #fff;
  font-size: 34rpx;

  color: rgb(51,51,51);
  line-height: 100rpx;
  height: 80rpx;
  margin: 0;
  padding: 0;
}
.searchInput::after{
  border:none;
}
.record{
  display: flex;
  flex-wrap: wrap;
}
.history_search{
  display: flex;
  height: 100rpx;
  padding: 0 20rpx;
  justify-content: space-between;
  align-items: center;
}
.history_tit{
color: rgb(51,51,51);
font-size: 28rpx;
}
.clear_history{
  width: 120rpx;
  height: 50rpx;
  line-height: 50rpx;
  background-color: rgb(247,247,247);
  text-align: center;
  font-size: 24rpx;
}
.cuIcon-search:before {
	content: "\e65c";
}
.text-search{
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 40rpx;
}
.search_type{
   display: inline;
  line-height: 50rpx;
  background-color: rgb(247,247,247);
  text-align: center;
  font-size: 24rpx;
  margin: 20rpx 20rpx;
}


.service_category {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
  margin-top: 15rpx;
  background-color: #fff;
  height: 80rpx;
  line-height: 80rpx;
}
.category_time{
  font-size: 22rpx;
}
.category_tit {
  color: rgb(34, 34, 34);
  font-size: 28rpx;
  font-weight: 700;
}

.category_product {
  margin-top: 2rpx;
  padding: 0 20rpx;
  flex-wrap:wrap;
  background-color: #fff;
  height: 180rpx;
  justify-content: space-between;
  align-items: center;
}

.product_img {
  width: 140rpx;
  height: 140rpx;
}

.product_img image {
  width: 140rpx;
  height: 140rpx;
}

.product_left {
  align-items: center;
  padding-left: 30rpx;
}

.product_name {
  padding-left: 30rpx;
  font-size: 26rpx;
  color: rgb(24, 24, 24);
  font-weight: 700;
}

.product_right {
  color: red;
  letter-spacing: 2rpx;
}


