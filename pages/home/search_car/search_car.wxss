  padding: 0 20rpx;
} */
.search{
  width: 100%;
  height: 90rpx;
  text-align: center;
  /* border: 1px solid rgb(247,247,247); */
  overflow: hidden;
}
.search input{
  /* margin-right: 20rpx; */
  text-align: center;
  border-radius: 30rpx;
  width: 100%;
  margin-top: 10rpx;
  height: 70rpx;
  background-color: #f7f7f7;
}



/* =============== */
/* pages/component/nav/nav.wxss */
.VerticalNav.nav {
  width: 200rpx;
  white-space: initial;
}

.VerticalNav.nav .cu-item {
  width: 100%;
  text-align: center;
  background-color: #fff;
  margin: 0;
  border: none;
  height: 50px;
  position: relative;
}

.VerticalNav.nav .cu-item.cur {
  background-color: #f1f1f1;
}
.VerticalNav.nav .cu-item.cur::after {
  content: "";
  width: 8rpx;
  height: 30rpx;
  border-radius: 10rpx 0 0 10rpx;
  position: absolute;
  background-color: currentColor;
  top: 0;
  right: 0rpx;
  bottom: 0;
  margin: auto;
}
.VerticalBox{
  display: flex;
}
.VerticalMain{
  background-color: #f1f1f1;
}