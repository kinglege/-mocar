/* pages/tobepaid/tobepaid.wxss */
.paid_address{
  background-color: #fff;
  padding: 0 20rpx;
}
.nav{
  margin-top: 6rpx;
}
.one{
  margin-top: 15rpx;
}
.address_img{
  width: 34rpx;
  height: 34rpx;
  margin-right: 20rpx;
}
.address_img image{
  width: 34rpx;
  height: 34rpx;
}
.paid_pro_state{
  padding-right: 4rpx;
}
/*弹框*/
.cu-modal.show{
  opacity:0.3;
}
.all{
  margin-bottom: 20rpx;
}
.tobepaid{
  margin: 2rpx 0;
  background-color: #fff;
  padding-right:20rpx; 
  height: 85rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.paid_address{
  display:flex;
  justify-content: center;
  align-items: center;
}
.address_name{
  padding-right: 30rpx;
  color: rgb(24,24,24);
  font-size: 24rpx;
  font-weight: 700;
}
.address_distance{
  color: rgb(191,191,191);
  font-size: 20rpx;
}
.address_time{
  letter-spacing: 2rpx;
  font-size: 24rpx;
}
.paid_pro_img{
  width: 140rpx;
  height: 140rpx;
}
.paid_pro{
  background-color: #fff;
  height: 200rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
}
.paid_pro_img image{
  width: 140rpx;
  height: 140rpx;
}
.name_n{
  color: rgb(24,24,24);
  font-weight: 700;
  padding-bottom: 45rpx;
}
.name_w{
  transform: translateX(90rpx);
  width: 77rpx;
  height: 26rpx;
  border: 1rpx solid rgb(242,120,61);
  text-align: center;
  line-height: 26rpx;
  font-size: 20rpx;
  color: rgb(242,120,61);

  padding-bottom: 30rpx;
}
.name_ww{
  transform: translateX(65rpx)
}
.name_b{
}
.aa{
  margin-top: 48rpx;
}
.name_h{
  color: rgb(12,12,12);
  font-size: 24rpx;
  font-weight: 700;
}
.name_s{
  color: rgb(236,44,64);
  font-size: 24rpx;
  font-weight: 700;
  line-height: 36rpx;
}
.footer{
  display: flex;
  height: 96rpx;
  margin-top: 3rpx;
  justify-content: space-around;
  align-items: center;
  background-color: #fff;
}
.footer_img{
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
}
.footer_phone{
  font-size: 20rpx;
  color: rgb(21,21,21);
  line-height: 38rpx;
}
.footer_img image{
  width: 30rpx;
  height: 30rpx;
}
.footer_cancel{
  width: 140rpx;
  height: 56rpx;
  border-radius: 10rpx;
  font-size: 24rpx;
  border: 2rpx solid rgb(100,100,100);
  text-align: center;
  line-height: 53rpx;
}
.footer_left{
  transform: translateX(50rpx);
}
.footer_payment{
  margin-left: 20rpx;
  border-radius: 10rpx;
  width: 140rpx;
  height: 56rpx;
  color: #fff;
  text-align: center;
  line-height: 53rpx;
 background: linear-gradient(to bottom,rgb(254,83,67),rgb(233,38,64));
}
.footer_right{
  width: 50%;
  display: flex;
  transform: translateX(55rpx);
  justify-content: flex-end;
  align-items: center;
  
}
/* 二底部部分 */

.contact {
  background-color: #fff;
  display: flex;
  width: 100%;
  height: 96rpx;
  margin-top: 4rpx;
}
.order_car{
  width: 238rpx;
}
.contact_left {
  height: 96rpx;
  width: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.img_left {
  width: 30rpx;
  height: 30rpx;
  margin-right: 15rpx;
}

.img_left image {
  width: 30rpx;
  height: 30rpx;
}
/* 退款售后修改样式 */
.name_refund{
  width: 128rpx;
  height: 50rpx;
  border: 2rpx solid rgb(100,100,100);
  font-size: 24rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius: 10rpx;
  transform: translate(40rpx,0rpx);
}
.ab{
  margin-top: 30rpx;
}
.name_cen{
  color: rgb(229,229,229);
  font-size: 26rpx;
  text-align: center;
  width: 128rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 10rpx;
  border: 2rpx solid rgb(229,229,229);
}