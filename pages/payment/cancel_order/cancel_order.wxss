  position: fixed;
  bottom: 0;
  background-color:#fff;
}
.accounts_left{
  font-size: 24rpx;
  justify-content: center;
  align-items: center;
}
.accounts_right{
  width: 260rpx;
  margin-top: 10rpx;
  height: 80rpx;
 border:2rpx solid rgb(100,100,100);
  font-size: 32rpx;
  line-height: 80rpx;
  
  text-align: center;
  transform: translateX(50%);
  border-radius: 10rpx;
}
.accounts_left_price{
  font-size: 30rpx;
  color: rgb(239,52,65);
}
.product_name{
  padding-bottom: 20rpx;
}
.product_tel{
  padding-left: 30rpx;
 font-size: 22rpx;
}
