.UCenter-bg{
  height: 400rpx;
  
}
.bg_header{
  height: 400rpx;
  padding-top: 100rpx;
  justify-content: center;
  align-items: center;
}
.bg_img{
  width: 200rpx;
  height: 200rpx;
  transform: translate()
}
.bg_img image{
  width: 200rpx;
  height: 200rpx;
}
.pay_txt{
  padding-left:150rpx;
