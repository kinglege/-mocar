/* === */

.order_info {
  height: 160rpx;
  background-color: #fff;
  justify-content: space-between;
  align-items: center;
}

.order_info_img {
  width: 63rpx;
  height: 63rpx;
  margin-top: 10rpx;
  margin-left: 20rpx;
}

.order_name, .order_car, .order_type {
  font-size: 26rpx;
  color: rgb(34, 34, 34);
  font-weight: 700;
}

.order_car{
  width: 140rpx;
  display: inline-block;
  vertical-align: top;
}
.order_type{
  display: inline-block;
  width: 450rpx;
  vertical-align: top;
}
.order_right {
  margin-bottom: 10rpx;
}

.order_tel {
  font-size: 26rpx;
  margin-top: 6rpx;
}

.order_info_name {
  padding-left: 20rpx;
}

.order_info_img image {
  width: 63rpx;
  height: 63rpx;
}

.order_icon {
  width: 18rpx;
  height: 23rpx;
}

.order_icon image {
  width: 18rpx;
  height: 23rpx;
}

/* 服务类目 */

.service_category {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
  margin-top: 15rpx;
  background-color: #fff;
  height: 80rpx;
  line-height: 80rpx;
}
.category_time{
  font-size: 22rpx;
  letter-spacing: 3rpx;
}
.category_tit {
  color: rgb(34, 34, 34);
  font-size: 28rpx;
  font-weight: 700;
}

.category_product {
  margin-top: 2rpx;
  padding: 0 20rpx;
  background-color: #fff;
  height: 180rpx;
  justify-content: space-between;
  align-items: center;
}

.product_img {
  width: 140rpx;
  height: 140rpx;
}

.product_img image {
  width: 140rpx;
  height: 140rpx;
}

.product_left {
  align-items: center;
  padding-left: 30rpx;
}

.product_name {
  padding-left: 30rpx;
  font-size: 26rpx;
  color: rgb(24, 24, 24);
  font-weight: 700;
}

.product_right {
  color: red;
  letter-spacing: 2rpx;
}

/* 线下门店 */

.store_tit {
  padding: 0 20rpx;
  margin-top: 15rpx;
  background-color: #fff;
  height: 80rpx;
  line-height: 80rpx;
  color: rgb(34, 34, 34);
  font-size: 28rpx;
  font-weight: 700;
}

.store_all{
  height: 130rpx;
  background-color: #fff;
  padding: 0 20rpx;
  justify-content: space-between;
  align-items: center;
}
.store_left_introduce{
  margin-left: 25rpx;
}
.store_left_top{
  padding-bottom: 15rpx;
}
.store_left_img{
  width: 90rpx;
  height: 90rpx;
}
.store_txt1{
  font-size: 26rpx;
  color: rgb(24,24,24);
  font-weight: 700;
}
.store_left_bottom{
  font-size: 22rpx;

}
.store_txt2{
  font-size: 22rpx;
  padding: 5rpx 0 0 15rpx;
}
.store_left_img image{
  width: 90rpx;
  height: 90rpx;
}
.store_right{
  width: 15rpx;
  height: 23rpx;
}
.store_right image{
  width: 15rpx;
  height: 23rpx;
}
/* 支付 */
.payment{
  padding: 0 20rpx;
  background-color: #fff;
}
.x2_img{
  color: rgb(252,78,67);
  font-size: 22rpx;
