}
.payment_one{
  padding-right: 30rpx;
  margin-top: 1rpx;
  height: 85rpx;
  line-height: 85rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.payment_two{
  margin-top: 1rpx;
  height: 85rpx;
  line-height: 85rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.x2_txt{
  padding-right: 20rpx;
}
.payment_three{
  margin-top: 1rpx;
  padding-right: 40rpx;
  height: 85rpx;
  line-height: 85rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.x1{
  color: rgb(34,34,34);
  font-size: 28rpx;
  font-weight: 700;
}
.x2{
  font-size: 22rpx;
}
/* 底部 */
.settle_accounts{
 
  width: 100%;
  display: flex;
  padding: 0 20rpx;
  justify-content: space-around;
