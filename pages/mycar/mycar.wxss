/* pages/mycar/mycar.wxss */
.page{
 width: 100%;
 height: 100%;
}
.qq{
   text-align: center;
  margin: 500rpx auto;
  display: flex;
  	align-items: center;
  	/* justify-content: center; */
  /* background-color:pink; */
    flex-direction: column;
}
.my_car{
  text-align: center;
  width: 100rpx;
  height: 100rpx;
}
image{
  vertical-align:middle;
  width: 100%;
  height: 100%;
}
/* 第一种情况 */
/* pages/myCar_info/myCar_info.wxss */
.car_info{
  height: 160rpx;
  background-color: #fff;
  margin-top: 6rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
}
.car_img{
  width: 100rpx;
  height: 100rpx;
}
.car_img image{
  width: 100rpx;
  height: 100rpx;
}
.top_txt1{
  padding-bottom: 20rpx;
  font-size: 26rpx;
  color: rgb(24,24,24);
  font-weight: 700;
}
.top_txt2{
  width: 120rpx;
  height: 26rpx;
  border: 1rpx solid rgb(236,104,65);
  text-align: center;
  line-height: 26rpx;
  color: rgb(236,104,65);
  transform: translateY(5rpx);
  font-size: 20rpx;
  margin-left: 30rpx;
}
.car_introduce{
  padding-left: 60rpx;
}

.top_txt4{
  padding-left: 10rpx;
}
.car_footer{
  padding: 0 20rpx;
  background-color: #fff;
  height: 90rpx;
  margin-top: 2rpx;
  justify-content: flex-end;
  align-items: center;
}

.car_default{
  width: 128rpx;
  height: 53rpx;
  border: 2rpx solid rgb(255,0,0);
  color: rgb(247,67,66);
  font-weight: 700rpx;
  margin-left: 25rpx;
  font-size: 24rpx;
  text-align: center;
  line-height: 53rpx;
  border-radius: 10rpx;
}
.state_car{
  background-color: #fff;
  height: 80rpx;
  justify-content: flex-end;
  align-items: center;
}
.delete,.default{
  width: 108rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;

}